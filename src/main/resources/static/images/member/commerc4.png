function getGnbBanner(pcYn) {
	return new Promise(function (resolve, reject) {
		const url = "/banner/gnbBannerAjax";

		if (pcYn == 'Y' && $('.swiper-common-banner .swiper-slide').length != 0
						|| $('.swiper-allmenu-banner .swiper-slide').length != 0) {
			return;
		}
		$.ajax({
			type: "POST",
			url: url,
			data: {pcgnb: (pcYn == 'Y') ? 'pcgnb' : ''},
			dataType: "html",
			success: function (data) {
				resolve(data);
			},
			error: function () {
				reject();
			}
		});
	});
}

