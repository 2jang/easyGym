		<script>
			let isMkPopupOpen = false;

			/**
			 * 마케팅 수신 동의 요청 팝업(mkPopup) 노출
			 *
			 * 마케팅 수신동의하지 않은 회원에게 팝업 노출
			 * 일주일 동안 보지 않기, 닫기(하루동안 보지 않기) 시, 미노출
			 * @return {Promise<string>}
			 * "00" : 팝업 노출 & 사용자에 의한 닫기 action
			 * "01" : 팝업 노출 & 사용자에 의한 나의 쿠폰 보러가기 action
			 * "90" : 팝업 미노출
			 */
			function showReqGrsMarktUsePop() {
				return new Promise(resolve => {
					if (grsMarktUseYn == 'Y' || !eatzPub.front.dialogOnOff().popOpen('#mkPopup')) {
						isMkPopupOpen = false;
						return resolve("90");
					} else {
						setCookieTimeZero("showMarktCnt", '1', 1);
						isMkPopupOpen = true;
					}

					const showMyCouponBtn = document.getElementById("goMyCouponBtn");
					showMyCouponBtn.addEventListener('click', function(e) {
						isMkPopupOpen = false;
						eatzPub.front.dialogOnOff().popClose('#mkPopup', null, null, 1);
						return resolve("01");
					});
					document.addEventListener('click', function(e) {
						if (isMkPopupOpen && checkClosedEvent(e.target)) {
							isMkPopupOpen = false;
							return resolve("00");
						}
					});
				});
			}

			function checkClosedEvent(target) {
				if (!document.querySelector('#mkPopup').contains(target)) {
					eatzPub.front.blockPop('mkPopup', 1);
					return true;
				}
				const closeBtns = document.querySelectorAll('.close-btn');
				for(const btn of closeBtns) {
					if (btn.contains(target)) {
						return true
					}
				}
				return false;
			}

			function agreeGrsMarktUse() {
				const param = [
					{AGRE_TYPE : "C3", AGRE_YN : "Y"},
					{AGRE_TYPE : "C6", AGRE_YN : "Y"},
					{AGRE_TYPE : "C7", AGRE_YN : "Y"}
				];

				$.ajax({
					type: "POST",
					url: "/member/grsMarktListUseYn",
					data: JSON.stringify(param),
					contentType: "application/json",
					success: function (data, status, xhr) {
						if (data && !isEmpty(data.marktRecvYn)) {
							const date = new Date();
							const agreDate = dateFormat(date, '.');

							if (data.msgFlg == "Y") {
								eatzPub.front.toastPopupActive('LOTTE EATZ 마케팅 활용 개인정보 수집 및 광고성 앱 푸시 수신에 동의되었습니다. (' + agreDate + ')');
							} else {
								eatzPub.front.toastPopupActive('LOTTE EATZ 광고성 앱 푸시 마케팅 알림이 수신처리 되었습니다. (' + agreDate + ')');
							}
							$("#grsMarktUseYn").prop("checked", true);
							$("#grsMarktUseYn").val(data.settingValue);
						}
					}, complete: function () {
						isMkPopupOpen = false;
						eatzPub.front.dialogOnOff().popClose('#mkPopup', null, null, 1);
					}
				});
			}
		</script>
	
