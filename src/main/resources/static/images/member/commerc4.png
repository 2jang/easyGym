<script>

function callAllMenu() {
	const isCalled = $("#allMenuBtn").attr("th-data-called");
	
	if (isCalled == "Y") {
		eatzPub.front.dialogOnOff().popOpen('#allmenuPopup');
	} else {
		const url = "/docker/allMenuAjax";
		$.ajax({
			type : "GET",
			url : url,
			dataType : "html",
			data : {bodyPath : window.location.pathname + window.location.search},
			success : function(data){
				$('#allmenuPopup').html(data);
				
				//배너 리스트 스와이프 세팅
				eatzPub.front.dialogOnOff().popOpen('#allmenuPopup', eatzPub.front.setMoAllmenu);
				$("#allMenuBtn").attr("th-data-called", "Y");
			},
			error : function(){
// 				console.error("처리중 오류가 발생하였습니다.");
			}
		});
		
	}
}

</script>


			
