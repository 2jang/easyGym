		/**
		 * [AppsFlyer] 인앱 이벤트 로깅
		 * @param event_data
		 */
		function logAfEvent(eventName, eventValues) {
			if (!eventName) {
				return;
			}

			try {
				if (window.AnalyticsWebInterface) {
					// Call Android interface
					window.AppsFlyerWebInterface.logEvent(eventName, JSON.stringify(eventValues))
				} else if (window.webkit
						&& window.webkit.messageHandlers
						&& window.webkit.messageHandlers.firebase) {
					// Call iOS interface
					const message = {
						command: 'logEvent',
						name: eventName,
						parameters: eventValues
					};
					window.webkit.messageHandlers.appsFlyer.postMessage(message);
				} else {
					// No Android or iOS interface found
					console.log("No native APIs found.");
				}
			} catch (e) {}
		}

		/**
		 * [AppsFlyer] 회원 가입 완료 로그
		 */
		function af_complete_registration() {
			try {
				const gno = null;
				const registDate = null;
				const certInfo = JSON.parse(localStorage.getItem("certInfo"));
				const afCRToday = JSON.parse(localStorage.getItem("afCRToday"));
				const today = new Date();

				if (!!certInfo && !!certInfo.certFrom && certInfo.certFrom != '' 			// 통합회원/카카오/네이버 구분값 존재여부
						&& (certInfo.certFrom == '통합회원' || certInfo.certYn == 'Y')			// 통합회원 or 자체회원 + 휴대전화 본인인증 완료
						&& (!registDate || registDate == '' || !!registDate && isSameDate(new Date(registDate.substring(0, 4) + "-" + registDate.substring(4, 6) + "-" + registDate.substring(6, 8)), today))	// 금일 회원가입 여부
						&& !!gno && gno != '' && (!afCRToday || (afCRToday.gno != gno || !isSameDate(new Date(afCRToday.date), today)))	// 신규 GNO이거나 동일한 GNO로 금일 중복 가입하지 않음
				) {
					logAfEvent('af_complete_registration', {
						af_registration_method: certInfo.certFrom,
						af_customer_user_id: gno,
						af_param_1: certInfo.certFrom
					});
					localStorage.setItem("afCRToday", JSON.stringify({"gno": gno, "date": today}));
				}
			} catch (e) {}
		}

		/**
		 * [AppsFlyer] 탈퇴 완료 로그
		 * @param gno 회원번호
		 * @param event_data
		 */
		function af_withdraw(gno, event_data) {
			if (!!gno && gno != "") {
				const date = new Date();
				logAfEvent('af_withdraw', {
					af_customer_user_id: gno,
					af_date_withdraw: date.getFullYear() + "-" + (date.getMonth() + 1) + "-" + date.getDate() + " " + date.getHours() + ":" + date.getMinutes()
				})
			}
		}

		/**
		 * 날짜 비교
		 */
		function isSameDate(date1, date2){
			return date1.getFullYear() === date2.getFullYear()
					&& date1.getMonth() === date2.getMonth()
					&& date1.getDate() === date2.getDate();
		}

