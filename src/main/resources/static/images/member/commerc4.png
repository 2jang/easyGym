//L.POINT 가져오기
function getLpointChk() {
	$.ajax({
		url: "/barcode/get/lpoint/ajax",
		type: "POST",
		contentType: "application/json",
		dataType: "json",
		success: function(res) {
			if (res.result == "SUCCESS") {
				let lpointFormat = res.lpoint.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");

				let lpointHtml = '';
				lpointHtml += '<span class="val">' + lpointFormat + '</span><span class="unit">P</span>';
				$("#lpointArea").html(lpointHtml);
			}
		},
		error : function(){
			createAlert("서버에서 처리중 오류가 발생했습니다. 문제가 계속 발생하면 관리자에게 문의하시기 바랍니다.");
		}
	});
}




