<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.isix.easyGym.payform.dao.PayformDAO">  <!-- namespace는 mapper의 이름을 정의 -->
 
 	<resultMap type="payformDTO" id="payformResult">
 		<result property="payformNo" column="payformNo"/>	<!--주문번호-->
 		<result property="memberNo" column="memberNo"/>	<!--멤버 번호-->
 		<result property="detailNo" column="detailNo"/>	<!--헬스장 번호-->
 		<result property="payformSub" column="payformSub"/>	<!--구독 개월-->
 		<result property="payformPrice" column="payformPrice"/>	<!--최종 금액-->
 		<result property="payformDate" column="payformDate"/>	<!--구매일, sysdate-->
 		<result property="payformStatus" column="payformStatus"/>	<!--주문상태-->
 		<result property="payformPayment" column="payformPayment"/>	<!--결제방법-->
		<result property="payformCancelDate" column="payformCancelDate"/>	<!--취소일, sysdate-->
